# So?  What about this header?
case $ac_header_compiler:$ac_header_preproc:$ac_cxx_preproc_warn_flag in
  yes:no: )
    { echo "$as_me:$LINENO: WARNING: fft.h: accepted by the compiler, rejected by the preprocessor!" >&5
echo "$as_me: WARNING: fft.h: accepted by the compiler, rejected by the preprocessor!" >&2;}
    { echo "$as_me:$LINENO: WARNING: fft.h: proceeding with the compiler's result" >&5
echo "$as_me: WARNING: fft.h: proceeding with the compiler's result" >&2;}
    ac_header_preproc=yes
    ;;
  no:yes:* )
    { echo "$as_me:$LINENO: WARNING: fft.h: present but cannot be compiled" >&5
echo "$as_me: WARNING: fft.h: present but cannot be compiled" >&2;}
